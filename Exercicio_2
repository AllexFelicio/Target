#include <stdio.h>
#include <locale.h>

main() {

    setlocale(LC_ALL, "Portuguese");
    
    int a, b, auxiliar, i, n;
    int verif;
    a = 0;
    b = 1;
    
    printf("Digite um número: ");
    scanf("%d", &n);


    for(i = 0; i < n; i++) {
        
        auxiliar = a + b;
        a = b;
        b = auxiliar;
        if(n==auxiliar)
        {
          verif = 1;  
        }
    }
    if(verif ==1)
    {
    printf("Tem na sequência");
        
    }
    else{
     printf("Não Tem na sequência");
        
    }   
    
}
